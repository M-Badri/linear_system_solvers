+LFLAGS = -qopenmp 
+LIBS = -L../linear_sys_lib/bin/Release -L../linear_sys_lib/third_party/lis/intel/lib -L../linear_sys_lib/third_party/zoo/intel/lib -L../linear_sys_lib/third_party/toast/intel/lib -llinear_sys_lib -llis -lzoo -ltoast 
